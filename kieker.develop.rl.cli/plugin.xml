<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         name="IRL CLI Compiler"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="kieker.develop.rl.cli.CompilerMain">
         </run>
      </application>
   </extension>
   <extension
         id="headless"
         point="org.eclipse.core.runtime.products">
      <product
            application="kieker.develop.rl.cli.id1">
      </product>
   </extension>
</plugin>
